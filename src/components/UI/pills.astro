---
import {cn} from '../../utils/index'
const {class:className, Tech = []} = Astro.props

---


  <div class={cn(className, 'flex flex-wrap gap-x-3')}>
    {
      Tech.map((tech:[])=>(
        <p class="px-3 py-1 font-regular tracking-wider bg-white/40 rounded-2xl">
        
          {tech}
          
          </p>
      ))
    }
  </div>
</div>