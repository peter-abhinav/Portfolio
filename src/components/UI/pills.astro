---
import {cn} from '../../utils/index'
const {class:className, Tech = []} = Astro.props

---


  <span class={cn(className, 'flex flex-wrap gap-x-5 gap-y-5 justify-center')}>
    {
      Tech.map((tech:[])=>(
        <p class="px-3 py-1 font-regular tracking-wider bg-white/10 hover:border-accent hover:text-accent rounded-2xl">
        
          {tech}
          
          </p>
      ))
    }
  </span>
</div>